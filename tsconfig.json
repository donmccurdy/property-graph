{
	"compilerOptions": {
		"paths": {
			"property-graph": ["./"]
		},
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"lib": ["ES2024"],
		"target": "ES2024",
		"isolatedModules": true,
		"allowJs": false,
		"declaration": true,
		"stripInternal": true,
		"strict": true,
		"erasableSyntaxOnly": true,
		"verbatimModuleSyntax": true,
		"useDefineForClassFields": true
	}
}
